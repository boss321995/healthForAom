    "start": "node server/index.js",
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "build:server": "npm run build && npm run copy:dist",
    "copy:dist": "if not exist server\\dist mkdir server\\dist && xcopy dist server\\dist\\ /E /I /Y || (mkdir -p server/dist && cp -r dist/* server/dist/)",
    "postinstall": "npm run build && npm run copy:dist",
    "server": "node server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy:vercel": "npm run build && vercel --prod",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=dist",
    "deploy:surge": "npm run build && surge dist",
    "deploy:gh-pages": "npm run build && gh-pages -d dist",
    "predeploy": "npm run build"
  },
